    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: image-analysis.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "cwd":"${workspaceFolder}/AZURE-AI-Vision/",
            "console": "integratedTerminal"
