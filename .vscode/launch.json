        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/AZURE-OPENAI/SEMANTIC-KERNEL-SDK/C#/SemanticKernel-Samples/bin/Debug/net8.0/SemanticKernel.dll",
            "args": [],
            "cwd": "${workspaceFolder}/AZURE-OPENAI/SEMANTIC-KERNEL-SDK/C#/SemanticKernel-Samples",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}